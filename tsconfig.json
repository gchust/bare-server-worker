{
	"compilerOptions": {
		"outDir": "dist",
		"types": [
			"@cloudflare/workers-types"
		],
		"lib": [
			"ESNext"
		],
		"module": "NodeNext",
		"moduleResolution": "NodeNext",
		"strict": true,
		"stripInternal": true,
		"esModuleInterop": true,
		"declaration": true,
		"sourceMap": true,
		"resolveJsonModule": false
	},
	"include": [
		"src"
	]
}